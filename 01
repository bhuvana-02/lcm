#include<stdio.h>
#include<conio.h>
void main()
{
	int n1,n2,lcm,gcd,a,b;
	printf("Enter two numbers to find lcm:\n");
	scanf("%d %d",&n1,&n2);
	a=n1;
	b=n2;
	while(n1!=n2)
	{
		if(n1>n2)
		{
			n1=n1-n2;
		}
		else
		{
			n2=n2-n1;
		}
	}
	gcd=n1;
	lcm=(a*b)/gcd;
	printf("LCM=%d\n",lcm);
	return 0;
	getch();
}
